                            },
                            {
                                "name": "Jurisdiction",
                                "type": "multiple_choice",
                                "editable": true,
                                "key": "jurisdiction",
                                "choices": customer.jurisdictions.filter(function(a)
                                {
                                    return a.active === true;
                                })
                                .map(function(a)
                                {
