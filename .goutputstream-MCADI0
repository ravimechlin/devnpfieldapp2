                                "type": "text",
                                "value": function(obj)
                                {
                                    return obj.customer_first_name + " " + obj.customer_last_name;
                                }

                            },
                            {
                                "name": "Customer Address",
                                "editable": true,
                                "type": "text",
                                "key": "customer_address",
                                "value": function(obj)
                                {
                                    return obj.customer_address;
                                },
                                "transform": function(txt)
                                {
                                    return $.trim(txt);
                                },
                                "validation": function(txt)
                                {
                                    return $.trim(txt).length > 0;
                                }
                            },
