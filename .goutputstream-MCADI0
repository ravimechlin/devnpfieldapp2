                                                        {
                                                                $("#to_do_item_" + parent_div.data("identifier")).attr("data-notes", notes2).data("notes", notes2);
                                                        }                                                    
                                                    });
                                                }
                                            }
                                        );
                                    }
                                    else
                                    {
