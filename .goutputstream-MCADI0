                                                }, "cust_comm", c_type, $("#sel_cust_response_note").val());
                                            }
                                        }
                                    }
                                ],
                                "onshown": function(dia)
                                {
                                    $("#pm_sel_comm_type").change(function()
                                    {
                                        if($(this).val() === "n/a")
                                        {
                                            $("#pm_other_hint").css("display", "block");
                                            $("#pm_other_type_input").css("display", "block");
                                        }
                                    });
                                }
                            }
                        )
