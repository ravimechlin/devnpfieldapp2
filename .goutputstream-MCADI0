                    //$("<br />").appendTo(questions_div);
                    //$("<br />").appendTo(questions_div);

                    $("<div></div>").addClass("dropdown").css("display", "none")
                    .append
                    (
                        $("<textarea></textarea>").attr("id", "roof_work_details").attr("placeholder", "Describe what type of roof/prep work is required...")
