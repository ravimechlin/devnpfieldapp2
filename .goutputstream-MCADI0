                                                                        {
                                                                            if(dialog_footer.find(".btn-primary").first().hasClass("hidden_btn_pop_up"))
                                                                            {
                                                                                dialog_footer.find(".btn-primary").first().removeClass("hidden_btn_pop_up");
                                                                            }  
