                    $("<span></span>").css("margin-left", "1em").css("cursor", "pointer").css("text-shadow", "0px 0px 0.1em #000000").addClass("label").addClass("label-primary").addClass("ed_export_label").text("Export")
                    .click(function()
                    {
                        var rows_arr = [];
