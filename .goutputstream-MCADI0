                                },
                                "transform": function(txt)
                                {
                                    var digits = txt.digits();
                                    if(digits !== null)
                                    {
                                        return digits;
                                    }
                                    return "";
                                },
                                "validation": function(txt)
                                {
                                    return txt.digits() !== null && txt.digits().length === 5;
                                }
