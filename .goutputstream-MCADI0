                            $.each(txt.split(""), function(i, char)
                            {
                                if(allowed_chars.indexOf(char) > -1)
                                {
                                    if(new_txt.indexOf(".") === -1 && char === ".")
                                    {
                                        new_txt += char;
                                    }
                                    else
                                    {
                                        if(char !== ".")
                                        {
                                            new_txt += char;
                                        }
                                    }
                                }
                            });
                            el.val(new_txt);
                        }

